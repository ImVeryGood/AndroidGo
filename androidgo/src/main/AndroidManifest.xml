<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.newdicooker.tempetek.androidgo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".com.all.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:overrideLibrary="io.fogcloud.sdk.easylink"
        tools:replace="android:allowBackup">
        <activity
            android:name=".MainActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".com.all.BaseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.all.ui.activity.WebActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.all.ui.activity.SearchResultActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.all.ui.activity.knowledge.KnowledgeActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>